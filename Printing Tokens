#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int main() {
    // Allocate a buffer
    char *s = malloc( (1001) * sizeof(char) );  // up to 1000 + '\0'
    if (s == NULL) {
        return 1;
    }
    // Read the full line (including spaces) until newline
    scanf("%[^\n]", s);

    // Optionally shrink memory
    s = realloc(s, strlen(s) + 1);

    // Use strtok to break on spaces
    char *token = strtok(s, " ");
    while (token != NULL) {
        printf("%s\n", token);
        token = strtok(NULL, " ");
    }

    free(s);
    return 0;
}
