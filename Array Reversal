#include <stdio.h>
#include <stdlib.h>   // for malloc, free if needed

void reverseArray(int *a, int n) {
    int start = 0;
    int end = n - 1;
    while (start < end) {
        int temp = a[start];
        a[start] = a[end];
        a[end] = temp;
        start++;
        end--;
    }
}

int main() {
    int n;
    scanf("%d", &n);

    // allocate array of size n
    int *arr = (int *) malloc(n * sizeof(int));
    if (arr == NULL) {
        return 1;  // allocation failed
    }

    for (int i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }

    reverseArray(arr, n);

    // print reversed array
    for (int i = 0; i < n; i++) {
        printf("%d", arr[i]);
        if (i < n - 1) {
            printf(" ");
        }
    }
    printf("\n");

    free(arr);
    return 0;
}
